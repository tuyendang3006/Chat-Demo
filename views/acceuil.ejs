<html>
    <head>
        <meta charset="utf-8">
        <title> Demo Socket IO</title> 
        <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
        <script src="https://jesmine-chat-socket.herokuapp.com/socket.io/socket.io.js"></script>
        <script src="xuly.js"></script> 
        <link rel="stylesheet" type="text/css" href="layout.css"/>     
    </head>

    <body>
        <div id="wrapper">

            <div id="loginForm">
                <h3>What's your name?</h3>
                <input type="text" id="txtUsername"/>
                <input type="button" value="Register" id="btnRegister"/>
            </div>

            <div id="chatForm">
                <div id="left">
                    <div id="boxTitle">User Online</div>
                    <div id="boxContent"></div>
                </div>
                <div id="right">
                    <div id="sayHi">
                        Hello <span id="currentUser">Tuyen</span>
                        <input type="button" id="btnLogout" value="Logout">
                    </div>
                    <div id="listMessages"></div>

                    <div id="annonce"></div>

                    <input type="text" id="textMessage"/>
                    <input type="button" id="btnSendMessage" value="Send"/>
                </div>
            </div>

        </div>
    </body>
</html>